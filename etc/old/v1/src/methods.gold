#!/usr/bin/env ../fun
# vim: filetype=awk ts=2 sw=2 sts=2  et :

@include "lib/gold.awk"

function adds(a,i,class,   j) {
  class=class?class:"Num"
  @class(i)
  for(j in a)
     add(i,a[j])
}
function add(i,x,  f) { f=method(i,"Add"); @f(i,x) }
function dec(i,x,  f) { f=method(i,"Dec"); @f(i,x) }
function mid(i,    f) { f=method(i,"Mid"); @f(i) }
function var(i,    f) { f=method(i,"Var"); @f(i) }

