function okmaths() {
  print("### -- okmaths")
  assert(4 ==abs(-5),"abs")
  assert(10==max(1,10),"max")
  assert(1 ==min(1,10),"min") }

function oklists(     a,b) {
  print("### -- oklists")
  print(5)
  new(a)
  print(10)
  push(10,a)
  return 1
  push(20,a)
  assert(2  == length(a),"push2")
  assert(20 == last(a),"last")
  copy(a,b)
  assert(2  == length(b),"copy")
  b[2] = 30
  assert(10 == b[1],"copy")
  append(a,b)
  oo(a) }

BEGIN { rogues(); okmaths() ;oklists() }
